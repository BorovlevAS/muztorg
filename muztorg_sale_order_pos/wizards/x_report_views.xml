<?xml version='1.0' encoding='utf-8' ?>
<odoo>
    <record id="xreport_wizard_view_form" model="ir.ui.view">
        <field name="name">xreport.wizard.view.form</field>
        <field name="model">xreport.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="report_data" />
                    </group>
                </sheet>
                <footer>
                    <button string="Close" class="btn-secondary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
